FOR LOOP

SET SERVEROUTPUT ON;
DECLARE
	I NUMBER(2);
BEGIN
	FOR I IN 1 .. 12 loop
		DBMS_OUTPUT.PUT_LINE(I||' * 19 = '|| I*19);
	END LOOP;
	
END;
/

-------------------------------------------------------------------------------------------------------------------------------------------------

WHILE LOOP

SET SERVEROUTPUT ON;
DECLARE
	I NUMBER(2) := 1;
BEGIN
	WHILE I <= 12 loop
		DBMS_OUTPUT.PUT_LINE(I||' * 19 = '|| I*19);
		I := I + 1;
	END LOOP;
	
END;
/


-------------------------------------------------------------------------------------------------------------------------------------------------
